{
  "name": "vrptw-ts",
  "description": "",
  "version": "1.0.3",
  "author": {
    "name": "Samuel Gratzl",
    "email": "sam@sgratzl.com",
    "url": "https://www.sgratzl.com"
  },
  "private": true,
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sgratzl/vrptw-ts/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sgratzl/vrptw-ts.git"
  },
  "sideEffects": [
    "*.scss",
    "*.css"
  ],
  "browserslist": [
    "Firefox >= 57",
    "Firefox ESR",
    "Edge >= 16",
    "Chrome >= 64"
  ],
  "main": "./src/index.js",
  "types": "./src/index.d.js",
  "files": [
    "build",
    "src/**/*.js",
    "src/**/*.d.ts",
    "src/assets",
    "src/**/*.scss"
  ],
  "scripts": {
    "clean": "rimraf build dist && npm run clean:compile",
    "clean:compile": "rimraf *.tsbuildinfo build/*.tsbuildinfo node_modules/.cache/cache-loader src/**/*.map src/**/*.js src/**/*.d.ts",
    "compile": "tsc -b",
    "compile:dev": "tsc -b ./tsconfig.dev.json",
    "compile:watch": "tsc -b ./tsconfig.dev.json -w",
    "lint": "tslint -p tsconfig.json -c ../../tslint.json && stylelint src/**/*.scss",
    "docs": "npm run clean:compile && typedoc --tsconfig tsconfig.dev.json --out ./build/docs/ src tsd.d.ts",
    "pretest": "npm run clean && npm run compile",
    "test": "echo no tests",
    "posttest": "npm run lint",
    "prebuild": "npm run clean && npm run test",
    "build:dev": "webpack --mode development --devtool source-map",
    "build:prod": "webpack --mode production --devtool source-map",
    "build": "npm run build:prod",
    "dev": "npm run clean && npm run compile:dev && npm run build:dev",
    "watch": "webpack --mode development --watch --devtool source-map",
    "start:client": "webpack-dev-server --mode development --devtool source-map --disable-host-check",
    "start:server": "minizinc-server --port 5000",
    "start": "concurrently npm:start:client npm:start:server",
    "predist": "npm run build",
    "dist": "echo dummy",
    "prepare": "echo 'dummy prepare since prepack has no dev dependencies'",
    "prepack": "npm run clean && npm run compile && npm run build:prod",
    "localtunnel": "lt --port 8080 --subdomain vrptw"
  },
  "devDependencies": {
    "cache-loader": "^3.0.0",
    "concurrently": "^4.1.0",
    "css-loader": "^2.1.1",
    "extract-loader": "^3.1.0",
    "file-loader": "^3.0.1",
    "fork-ts-checker-webpack-plugin": "^1.2.0",
    "html-webpack-plugin": "^3.2.0",
    "json-loader": "^0.5.7",
    "localtunnel": "^1.9.1",
    "mini-css-extract-plugin": "^0.6.0",
    "minizinc-server": "^1.0.4-alpha.5",
    "mkdirp": "^0.5.1",
    "node-sass": "^4.12.0",
    "raw-loader": "^2.0.0",
    "rimraf": "^2.6.3",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "stylelint": "^10.0.1",
    "stylelint-config-standard": "^18.3.0",
    "stylelint-scss": "^3.6.1",
    "thread-loader": "^2.1.2",
    "ts-loader": "^5.4.5",
    "tslint": "^5.16.0",
    "tslint-consistent-codestyle": "^1.15.1",
    "tslint-eslint-rules": "^5.4.0",
    "typedoc": "^0.14.2",
    "typescript": "^3.4.5",
    "url-loader": "^1.1.2",
    "webpack": "~4.28.4",
    "webpack-cli": "^3.3.1",
    "webpack-dev-server": "^3.3.1"
  },
  "dependencies": {
    "@material-ui/core": "^4.0.1",
    "@material-ui/icons": "^4.0.1",
    "@material-ui/lab": "^4.0.0-alpha.14",
    "@types/classnames": "^2.2.7",
    "@types/d3": "^5.7.2",
    "@types/react": "^16.8.19",
    "@types/react-dom": "^16.8.4",
    "classnames": "^2.2.6",
    "d3": "^5.9.2",
    "decko": "^1.2.0",
    "minizinc": "^1.0.4-alpha.5",
    "mobx": "^5.9.4",
    "mobx-react": "^5.4.4",
    "react": "^16.8.6",
    "react-async-component": "^2.0.0",
    "react-dom": "^16.8.6",
    "typeface-roboto": "0.0.54"
  }
}
